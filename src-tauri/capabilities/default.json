{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:webview:allow-set-webview-auto-resize",
    "core:webview:allow-set-webview-zoom",
    "core:window:allow-set-size"
  ]
}